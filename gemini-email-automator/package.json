{
  "name": "gemini-email-automator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "tsx watch --env-file=.env.local server.ts",
    "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
    "build:client": "vite build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm run build:client && npm run build:server",
    "start": "node dist-server/server.js"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "body-parser": "^2.2.0",
    "@types/body-parser": "^1.19.5",
    "cors": "^2.8.5",
    "@types/cors": "^2.8.19",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "nodemailer": "^6.9.14",
    "@types/nodemailer": "^6.4.15",
    "openai": "^4.52.7",
    "papaparse": "^5.4.1",
    "@types/papaparse": "^5.3.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "pg": "^8.12.0",
    "@types/pg": "^8.11.6"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": "22.x"
  }
}
