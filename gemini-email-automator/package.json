  {
    "name": "gemini-email-automator",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
      "dev:client": "vite",
      "dev:server": "tsx watch --env-file=.env.local server.ts",
      "dev": "concurrently \"npm:dev:client\" \"npm:dev:server\"",
      "build:client": "vite build",
      "build:server": "tsc --project tsconfig.server.json",
      "build": "npm run build:client && npm run build:server",
      "start": "node dist-server/server.js"
    },
    "dependencies": {
      "@types/papaparse": "^5.3.14",
      "axios": "^1.7.2",
      "body-parser": "^2.2.0",
      "cors": "^2.8.5",
      "dotenv": "^16.4.5",
      "express": "^4.21.2",
      "nodemailer": "^7.0.9",
      "openai": "^4.52.7",
      "papaparse": "^5.4.1",
      "pg": "^8.16.3",
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "devDependencies": {
      "@types/body-parser": "^1.19.6",
      "@types/cors": "^2.8.19",
      "@types/express": "^5.0.3",
      "@types/node": "^24.7.0",
      "@types/nodemailer": "^7.0.2",
      "@types/pg": "^8.15.5",
      "@types/react": "^18.3.3",
      "@types/react-dom": "^18.3.0",
      "@vitejs/plugin-react": "^5.0.0",
      "concurrently": "^8.2.2",
      "ts-node": "^10.9.2",
      "tsx": "^4.20.6",
      "typescript": "~5.6.3",
      "vite": "^6.2.0"
    },
    "engines": {
      "node": "22.x"
    }
